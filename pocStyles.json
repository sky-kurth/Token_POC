{
  "global": {
    "fontFamilies": {
      "source-sans-pro": {
        "$type": "fontFamilies",
        "$value": "Source Sans Pro"
      },
      "roboto": {
        "$type": "fontFamilies",
        "$value": "Roboto"
      }
    },
    "lineHeights": {
      "auto": {
        "$type": "lineHeights",
        "$value": "AUTO"
      },
      "150%": {
        "$type": "lineHeights",
        "$value": "150%"
      },
      "125%": {
        "$type": "lineHeights",
        "$value": "125%"
      }
    },
    "fontSize": {
      "sm": {
        "$type": "fontSizes",
        "$value": "{fontSize.rem} *0.875"
      },
      "rem": {
        "$type": "fontSizes",
        "$value": "18"
      },
      "lg": {
        "$type": "fontSizes",
        "$value": "{fontSize.rem} *1.125"
      },
      "title": {
        "$type": "fontSizes",
        "$value": "{fontSize.rem} *2.063"
      },
      "xs": {
        "$type": "fontSizes",
        "$value": "{fontSize.rem} *0.75"
      }
    },
    "letterSpacing": {
      "0": {
        "$type": "letterSpacing",
        "$value": "0%"
      }
    },
    "paragraphSpacing": {
      "0": {
        "$type": "paragraphSpacing",
        "$value": "0"
      }
    },
    "Header": {
      "H1": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{fontFamilies.source-sans-pro}",
          "fontWeight": "{normal}",
          "lineHeight": "{lineHeights.auto}",
          "fontSize": "{fontSize.title}",
          "letterSpacing": "{letterSpacing.0}",
          "paragraphSpacing": "{paragraphSpacing.0}",
          "paragraphIndent": "{paragraphIndent.0}",
          "textCase": "{textCase.none}",
          "textDecoration": "{textDecoration.none}"
        }
      },
      "H2": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{fontFamilies.source-sans-pro}",
          "fontWeight": "{semibold}",
          "lineHeight": "{lineHeights.auto}",
          "fontSize": "{fontSize.lg}",
          "letterSpacing": "{letterSpacing.0}",
          "paragraphSpacing": "{paragraphSpacing.0}",
          "paragraphIndent": "{paragraphIndent.0}",
          "textCase": "{textCase.none}",
          "textDecoration": "{textDecoration.none}"
        }
      },
      "H3": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{fontFamilies.source-sans-pro}",
          "fontWeight": "{semibold}",
          "lineHeight": "{lineHeights.auto}",
          "fontSize": "{fontSize.rem}",
          "letterSpacing": "{letterSpacing.0}",
          "paragraphSpacing": "{paragraphSpacing.0}",
          "paragraphIndent": "{paragraphIndent.0}",
          "textCase": "{textCase.none}",
          "textDecoration": "{textDecoration.none}"
        }
      },
      "H4": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{fontFamilies.source-sans-pro}",
          "fontWeight": "{semibold}",
          "lineHeight": "{lineHeights.auto}",
          "fontSize": "{fontSize.sm}",
          "letterSpacing": "{letterSpacing.0}",
          "paragraphSpacing": "{paragraphSpacing.0}",
          "paragraphIndent": "{paragraphIndent.0}",
          "textCase": "{textCase.none}",
          "textDecoration": "{textDecoration.none}"
        }
      }
    },
    "p": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{fontFamilies.roboto}",
        "fontWeight": "{normal}",
        "lineHeight": "{lineHeights.auto}",
        "fontSize": "{fontSize.sm}",
        "letterSpacing": "{letterSpacing.0}",
        "paragraphSpacing": "{paragraphSpacing.0}",
        "paragraphIndent": "{paragraphIndent.0}",
        "textCase": "{textCase.none}",
        "textDecoration": "{textDecoration.none}"
      }
    },
    "text": {
      "base": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{fontFamilies.roboto}",
          "fontWeight": "{normal}",
          "lineHeight": "{lineHeights.150%}",
          "fontSize": "{fontSize.rem}",
          "letterSpacing": "{letterSpacing.0}",
          "paragraphSpacing": "{paragraphSpacing.0}",
          "paragraphIndent": "{paragraphIndent.0}",
          "textCase": "{textCase.none}",
          "textDecoration": "{textDecoration.none}"
        }
      },
      "sm": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{fontFamilies.roboto}",
          "fontWeight": "{normal}",
          "lineHeight": "{lineHeights.125%}",
          "fontSize": "{fontSize.sm}",
          "letterSpacing": "{letterSpacing.0}",
          "paragraphSpacing": "{paragraphSpacing.0}",
          "paragraphIndent": "{paragraphIndent.0}",
          "textCase": "{textCase.none}",
          "textDecoration": "{textDecoration.none}"
        }
      },
      "xs": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{fontFamilies.roboto}",
          "fontWeight": "{normal}",
          "lineHeight": "{lineHeights.auto}",
          "fontSize": "{fontSize.xs}",
          "letterSpacing": "{letterSpacing.0}",
          "paragraphSpacing": "{paragraphSpacing.0}",
          "paragraphIndent": "{paragraphIndent.0}",
          "textCase": "{textCase.none}",
          "textDecoration": "{textDecoration.none}"
        }
      }
    },
    "textCase": {
      "none": {
        "$type": "textCase",
        "$value": "none"
      }
    },
    "textDecoration": {
      "none": {
        "$type": "textDecoration",
        "$value": "none"
      }
    },
    "paragraphIndent": {
      "0": {
        "$type": "dimension",
        "$value": "0px"
      }
    },
    "medium": {
      "$type": "fontWeights",
      "$value": "500"
    },
    "semibold": {
      "$type": "fontWeights",
      "$value": "600"
    },
    "bold": {
      "$type": "fontWeights",
      "$value": "700"
    },
    "normal": {
      "$type": "fontWeights",
      "$value": "400"
    },
    "light": {
      "$type": "fontWeights",
      "$value": "300"
    },
    "black": {
      "$type": "fontWeights",
      "$value": "900"
    },
    "link": {
      "base": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{fontFamilies.roboto}",
          "fontWeight": "{medium}",
          "lineHeight": "{lineHeights.150%}",
          "fontSize": "{fontSize.rem}",
          "letterSpacing": "{letterSpacing.0}",
          "paragraphSpacing": "{paragraphSpacing.0}",
          "paragraphIndent": "{paragraphIndent.0}",
          "textCase": "{textCase.none}",
          "textDecoration": "{textDecoration.none}"
        }
      },
      "sm": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{fontFamilies.roboto}",
          "fontWeight": "{medium}",
          "lineHeight": "{lineHeights.125%}",
          "fontSize": "{fontSize.sm}",
          "letterSpacing": "{letterSpacing.0}",
          "paragraphSpacing": "{paragraphSpacing.0}",
          "paragraphIndent": "{paragraphIndent.0}",
          "textCase": "{textCase.none}",
          "textDecoration": "{textDecoration.none}"
        }
      },
      "xs": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{fontFamilies.roboto}",
          "fontWeight": "{medium}",
          "lineHeight": "{lineHeights.auto}",
          "fontSize": "{fontSize.xs}",
          "letterSpacing": "{letterSpacing.0}",
          "paragraphSpacing": "{paragraphSpacing.0}",
          "paragraphIndent": "{paragraphIndent.0}",
          "textCase": "{textCase.none}",
          "textDecoration": "{textDecoration.none}"
        }
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "global"
    ]
  }
}
